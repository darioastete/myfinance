{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@common/*": [
        "./src/common/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@utils/*": [
        "./src/common/utils/*"
      ],
      "@const/*": [
        "./src/common/const/*"
      ]
    },
    "jsx": "preserve"
  }
}